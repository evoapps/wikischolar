digraph {
    label="Results of BRD cycle editing";
    fontname=Helvetica;
    labelloc=t;
    rankdir=LR;
    node[shape=circle; fontname=helvetica, style=filled];
    edge[dir=none];

    x -> y[dir=forward];
    y -> {a, b, c, d};
    c -> z;
    z -> o[dir=forward];

    x[style=invis];
    y[label=""];
    z[label=""];
    o[style=invis];

    // Parent
    y[fillcolor="#8da0cb"];

    // Selected
    c[fillcolor="#8da0cb"];

    // Extant
    z[fillcolor="#8da0cb"];
}
